				"SiRsv30": "RSV_30天",
				"SiK5": "K_5天",
				"SiK30": "K_30天",
				"SiD5": "D_5天",
				"SiD30": "D_30天",
				"SiLongEma": "EMA長 (指數移動平均線 - 長期)",
				"SiShortEma": "EMA短 (指數移動平均線 - 短期)",
				"SiDif": "DIF (差離值)",
				"SiMacd": "MACD",
				"SiOsc": "OSC",
				"SiMa": "OSC_MA"
			};
			const foundmentParam = {
				"SbYield": "殖利率",
				"SbPbratio": "淨值比",
				"SbEps": "EPS",
				"SbBussinessIncome": "營業收入",
				"SiPe": "PE (市盈率)"
			};
