				var paras = GetPageAllNeedValue();
				// 根據不同模型做不同事情
				switch (paras.usingModel) {
					case "R":
						RegModelBuilding(paras);
						break;
					case "T":
						TimeModelBuilding(paras);
						break;
					default:
